  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/register": "7.25.9",
    "babel-loader": "9.2.1",
    "btoa": "1.2.1",
    "date-format": "4.0.14",
